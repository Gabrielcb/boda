CUCL_GLOBAL_KERNEL void %(rtc_func_name)( GASQ float const * const ins_0, // CUCL IN img:chan:y:x
					  GASQ float const * const ins_1, // CUCL IN img:chan:y:x
					  GASQ float * const out ) // CUCL OUT img:chan:y:x
{
  // CUCL IX GLOB_ID_1D out
  if( GLOB_ID_1D >= %(out_dims_prod) ) { return; }
  float v = 0;
  v += ins_0[GLOB_ID_1D];
  v += ins_1[GLOB_ID_1D];
  out[GLOB_ID_1D] = v;
}

